{
  "name": "url_fixer",
  "version": "1.0.0",
  "description": "A web service that validates and fixes URLs in documents using intelligent search and replacement",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "proxy": "node proxy-server.js",
    "dev:full": "concurrently \"npm run proxy\" \"npm run dev\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint src --ext .js",
    "format": "prettier --write src/**/*.{js,css,html}"
  },
  "keywords": ["url", "validation", "document", "web", "service", "search", "replacement"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@vitejs/plugin-legacy": "^7.0.0",
    "cors": "^2.8.5",
    "dompurify": "^3.2.6",
    "express": "^4.21.2",
    "jszip": "^3.10.1",
    "mammoth": "^1.9.1",
    "marked": "^16.0.0",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "vite": "^7.0.0"
  },
  "devDependencies": {
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.1.0",
    "eslint": "^9.30.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "vitest": "^3.2.4"
  }
}
